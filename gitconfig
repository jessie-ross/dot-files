# This is Git's per-user configuration file.
[user]
	name = Jessie Ross
	email = j@scjr.me
[includeIf "gitdir:~/code/"]
	path = ~/code/.gitconfig
[core]
    excludesfile = ~/.gitignore.global
[push]
	default = current
[pull]
	ff = only
[alias]
	co = checkout
	cp = cherry-pick
	sw = switch
	st = status
	fa = fetch --all
	c = commit
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
